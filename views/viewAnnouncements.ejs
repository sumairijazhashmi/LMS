
<%-include("partials/header");-%>

    <body class="assignmentsTab"
        style="font-size: 2em; width: 100%; height: 100vh; display: flex; justify-content: center; align-items: center; flex-direction: column;">
        <h1 style="left:20px;">Announcements</h1>
        <table class="table table-striped table-bordered table-hover">
            <thead>
                <tr>
                    <th scope="col">Announcements Title</th>
                    <th scope="col">Details</th>
                    <th scope="col">Modified Date</th>
                </tr>
            </thead>
            <tbody>
                <% Object.keys(announcements).forEach(function(x){ %>
                <tr> 
                    <td>
                        <%=announcements[x].heading%>
                    </td>
                    <td>
                        <%=announcements[x].body_text%>
                    </td>
                    <td>
                        <%=announcements[x].modified_date%>
                    </td>
                </tr>
                <% }); %>
            </tbody>
        </table>


    </body>

    </html>