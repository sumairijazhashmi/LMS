<%-include("partials/header");-%>
<body class="mainpage" style="height: 100vh; display: flex; flex-direction: column; justify-content: center; align-items: center;">
    <link rel="stylesheet" href="/css/styles.css" type="text/css">

    <form action="/postFeedback" method="POST">
        <div class="form-outline mb-4">
            <h3>User ID</h3>
            <input  class="form-control" type="text" placeholder="User" name="userID">
        </div>
        <div class="form-outline mb-4">
            <h3>Due Date</h3>
            <input  class="form-control" type="datetime-local" name="due_date">
        </div>
        
            <h3>Score</h3>
            <input class="form-control" type="number" step="0.01" placeholder="Score" name="score">
        </div>
        <br><br>
        <button type="submit">Release Score</button>    
        <br>
    </form>
    <br><br>
    <h5><%=message%></h5>
    <!-- <<script src="/js/index.js"></script> -->
</body>
</html>