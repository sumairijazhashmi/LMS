<%-include("partials/header");-%>
<body>
    <div class="container mt-5 px-5">
        <table class="table table-striped table-hover" style="font-size: 16px;">
            <thead>
              <tr>
                <th scope="col">Student IDs</th>
              </tr>
            </thead>
            <tbody>
              <% for(let i=0;i<data.length;i++) {%>
              <tr>
                <td><%=data[i].student_id%></td>
              </tr>
              <% } %>
            </tbody>
          </table>
          <div class="display-6 justify-content-center align-items-center mt-3 py-2" style="<%=noRecords%>; font-size: 15px; background-color: #afe0bd;">No Student Found</div>
    </div>

</body>
</html>